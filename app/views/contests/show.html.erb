<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @contest.name %>
</p>

<p>
  <strong>Start date:</strong>
  <%= @contest.start_date %>
</p>

<p>
  <strong>End date:</strong>
  <%= @contest.end_date %>
</p>
<strong>Questions:</strong>
<ol>
<% for quiz in @contest.quizzes %>
<li> <%= quiz.ques_content %> </li>
<ul>
<li> <%= quiz.opt_A %></li>
 <li><%= quiz.opt_B %></li>
<li> <%= quiz.opt_c %></li>
<li> <%= quiz.opt_d %></li>
</ul>
<% end %>
</ol>
<%= link_to 'Edit', edit_contest_path(@contest) %> |
<%= link_to 'Back', contests_path %>
